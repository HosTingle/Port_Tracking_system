@model dneme2.Models.YükBilgi

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<hr />
<hr />
<hr />
<div class="container">
    <div class="d-flex justify-content-center h-50">
        <h1>Liman Kaydı Silme</h1>
        <br />
    </div>
</div>
<hr />
<hr />
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            Yül Ağırlığı
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YükAgir)
        </dd>
        <dt class="col-sm-2">
            Yük Adedi
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YükMiktar)
        </dd>
        <dt class="col-sm-2">
            Yük Güvenliği
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YükGüven)
        </dd>
        <dt class="col-sm-2">
            Yük Türü
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YukTur)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdYükbil" />
        <input type="submit" value="Sil" class="btn btn-danger" /> |
        <a asp-action="Index">Yük Bilgilerine Dön</a>
    </form>
</div>
